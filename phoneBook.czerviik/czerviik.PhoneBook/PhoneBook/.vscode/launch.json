{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (PhoneBook)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net9.0/PhoneBook.dll",
      "cwd": "${workspaceFolder}",
      "console": "externalTerminal",
      "stopAtEntry": false,
      "justMyCode": true
    }
  ]
}
